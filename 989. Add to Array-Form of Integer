#1
class Solution:
    def addToArrayForm(self, num, k):
        sys.set_int_max_str_digits(10001)
        return list(map(int, list(str(int(''.join(list(map(str, num)))) + k))))

#2 Human form
class Solution:
    def addToArrayForm(self, num, k):
        sys.set_int_max_str_digits(10001)
        num = list(map(str, num))
        num = ''.join(num)
        num = int(num) + k
        num = list(map(int, list(str(num))))
        return num
